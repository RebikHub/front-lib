{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["auto-import"],
  "rules": {
    "auto-import/auto-import": [
      2,
      {
        "rootPath": "./"
      }
    ],
    "no-unused-vars": "warn",
    "linebreak-style": "off"
  }
}
